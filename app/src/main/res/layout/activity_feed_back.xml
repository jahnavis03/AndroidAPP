<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".FeedBack_Activity">

    <EditText
        android:id="@+id/feed_name"
        android:layout_width="336dp"
        android:layout_height="61dp"
        android:layout_marginTop="92dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.387"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/feedback_msg"
        android:layout_width="335dp"
        android:layout_height="65dp"
        android:layout_marginTop="52dp"
        android:ems="10"
        android:hint="Feedback"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/feed_name"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/feed_name"
        app:layout_constraintTop_toBottomOf="@+id/feed_name" />

    <Button
        android:id="@+id/se_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:text="Send feedback"
        app:layout_constraintEnd_toEndOf="@+id/feedback_msg"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="@+id/feedback_msg"
        app:layout_constraintTop_toBottomOf="@+id/feedback_msg" />
</androidx.constraintlayout.widget.ConstraintLayout>